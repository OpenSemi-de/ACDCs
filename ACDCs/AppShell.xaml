<?xml version="1.0" encoding="UTF-8"?>

<Shell
    x:Class="ACDCs.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="using:ACDCs.Views"
    Shell.FlyoutBehavior="Flyout"
    Shell.Title=""
    NavBarIsVisible="False">
    <ShellContent
        Route="start"
        Title="StartCenter"
        ContentTemplate="{DataTemplate views:StartCenterPage}" />
    <ShellContent
        Route="circuit"
        Title="New circuit"
        ContentTemplate="{DataTemplate views:CircuitSheetPage}" />
</Shell>
