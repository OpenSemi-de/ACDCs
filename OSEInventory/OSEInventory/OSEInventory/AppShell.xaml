<?xml version="1.0" encoding="utf-8" ?>

<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="clr-namespace:OSEInventory"
       x:Class="OSEInventory.AppShell"
       FlyoutBehavior="Disabled">

    <Shell.MenuBarItems>
        <MenuBarItem Text="File">
            <MenuFlyoutSubItem Text="New">
                <MenuFlyoutItem Text="Project"></MenuFlyoutItem>
            </MenuFlyoutSubItem>
            <MenuFlyoutItem Text="Open..."></MenuFlyoutItem>
            <MenuFlyoutItem Text="Save"></MenuFlyoutItem>
            <MenuFlyoutItem Text="Save as..."></MenuFlyoutItem>
        </MenuBarItem>
        <MenuBarItem Text="Edit">
            <MenuFlyoutItem Text="Undo"></MenuFlyoutItem>
            <MenuFlyoutItem Text="Redo"></MenuFlyoutItem>
            <MenuFlyoutItem ></MenuFlyoutItem>
            <MenuFlyoutItem Text="Copy"></MenuFlyoutItem>
            <MenuFlyoutItem Text="Cut"></MenuFlyoutItem>
            <MenuFlyoutItem Text="Paste"></MenuFlyoutItem>
        </MenuBarItem>
        <MenuBarItem  Text="Insert" />
    </Shell.MenuBarItems>
    <ShellContent Title="Welcome" ContentTemplate="{DataTemplate views:WelcomePage}"></ShellContent>
</Shell>
